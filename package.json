{
  "scripts": {
    "open": "npx cypress open",
    "visual": "sudo npx playwright test",
    "update visual": "npx playwright test --update-snapshots",
    "cloud": "npx cypress run --record --key c5703e87-4513-49af-84f9-5cd689ac8e00 --spec 'cypress/e2e/tests_regression/*.cy.js' --browser chrome",
    "e2e": "CYPRESS_EXCLUDE_TAGS=Bug npx cypress run --spec 'cypress/e2e/tests_e2e/*.cy.js' --browser chrome",
    "integration": "CYPRESS_EXCLUDE_TAGS=Bug npx cypress run --spec 'cypress/e2e/tests_integration/*.cy.js' --browser chrome",
    "regression": "CYPRESS_EXCLUDE_TAGS=Bug npx cypress run --spec 'cypress/e2e/tests_regression/*.cy.js' --browser chrome --headless",
    "api": "CYPRESS_EXCLUDE_TAGS=Bug npx cypress run --spec 'cypress/e2e/tests_api/*.cy.js' --browser chrome",
    "bug": "CYPRESS_INCLUDE_TAGS=Bug npx cypress run --browser chrome --config retries.runMode=0,retries.openMode=0",
    "lighthouse": "npm install -g lighthouse-viewer && node cypress/e2e/tests_lighthouse/LighthouseTests.js && node cypress/e2e/tests_lighthouse/MergeLighthouseReports.js",
    "one:test": "npx cypress run --spec $npm_config_file",
    "accessibility": "npx playwright test 'playwright/tests/AccessibilityTests.spec.ts'",
    "merge:reports": "jrm automation-report.xml \"results/*.xml\"",
    "merge:e2ereport": "jrm qa-e2e-test-report.xml \"results/*.xml\"",
    "merge:integrationreport": "jrm qa-integration-test-report.xml \"results/*.xml\"",
    "merge:regressionreport": "jrm qa-regression-test-report.xml \"results/*.xml\"",
    "merge:bugreport": "jrm qa-bug-test-report.xml \"results/*.xml\"",
    "merge:apireport": "jrm qa-api-test-report.xml \"results/*.xml\"",
    "merge:lighthousereport": "jrm qa-lighthouse-test-report.html \"cypress/reports/lighthouse/*.html\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:http": "node dist/index.js --transport=http",
    "start:stdio": "node dist/index.js --transport=stdio",
    "dev": "ts-node-esm src/index.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@aws-amplify/cli": "^12.11.1",
    "@axe-core/playwright": "^4.10.1",
    "@faker-js/faker": "^9.1.0",
    "@playwright/test": "^1.51.1",
    "@types/node": "22.13.11",
    "axe-core": "^4.9.0",
    "axe-html-reporter": "^2.2.11",
    "axe-playwright": "^2.1.0",
    "cypress": "^14.5.1",
    "cypress-file-upload": "^5.0.8",
    "cypress-plugin-api": "^2.11.1",
    "cypress-plugin-tab": "^1.0.5",
    "cypress-tags": "^1.2.2",
    "faker": "^5.5.3",
    "junit-report-merger": "^6.0.2",
    "mochawesome": "^7.1.3",
    "puppeteer": "^22.10.0",
    "typescript": "^5.4.5",
    "@types/cors": "^2.8.17",
    "@types/express": "5.0.1",
    "@types/jest": "^29.5.11",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^6.3.12",
    "bluebird": "^3.7.2",
    "mocha-junit-reporter": "^2.2.1",
    "@modelcontextprotocol/sdk": "1.10.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "^14.5.1",
    "cors": "^2.8.5",
    "cypress": "14.3.2",
    "esbuild": "0.25.3",
    "express": "5.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "typescript": "5.8.3",
    "zod": "3.24.3"
  },
  "name": "mcp-frontend-testing",
  "version": "1.0.0",
  "description": "MCP server for frontend testing with Jest and Cypress",
  "main": "dist/index.js",
  "type": "module"
}